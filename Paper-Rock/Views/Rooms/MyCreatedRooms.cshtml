@model IEnumerable<Paper_Rock.Models.Room>
<h2>List of your open rooms</h2>

<table class="table">
    <tr>
       

        <th>
          Room 
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
          
            <td>
            Room nr @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
          
        </tr>
    }

    </table>

<hr />
